# YOGAAROGYA Frontend Environment Variables
# Copy this to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# App Configuration
VITE_APP_NAME=YOGAAROGYA
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Personalized Yoga Recommendations

# API Configuration (if using custom backend)
VITE_API_BASE_URL=https://api.yogaarogya.com

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_SOCIAL_AUTH=true
VITE_ENABLE_VIDEO_TUTORIALS=true

# External Services
VITE_YOUTUBE_API_KEY=your_youtube_api_key_here
